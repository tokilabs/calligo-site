    "@testing-library/user-event": "^13.5.0",
    "bfj": "^7.0.2",
    "chalk": "^v4.1.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "inquirer": "^8.2.4",
    "nodemon": "^3.0.3"
  },
  "resolutions": {
    "moment": "^2.30.1",
    "react-dates/lodash": "^4.17.21",
    "react-test-renderer": "^16.13.1"
  },
  "nodemonConfig": {
    "execMap": {
      "js": "node --icu-data-dir=node_modules/full-icu"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.18.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "proseWrap": "always"
  },
  "packageManager": "pnpm@8.11.0"
}
