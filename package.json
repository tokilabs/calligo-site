{
  "name": "calligo-site",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "st:audit": "cd src/sharetribe; pnpm audit --json | node scripts/audit.js",
    "st:clean": "cd src/sharetribe; rm -rf build/*",
    "st:config": "cd src/sharetribe; node scripts/config.js",
    "st:config-check": "cd src/sharetribe; node scripts/config.js --check",
    "st:dev-frontend": "cd src/sharetribe; sharetribe-scripts start",
    "st:dev-backend": "cd src/sharetribe; nodemon server/apiServer.js",
    "st:dev": "cd src/sharetribe; pnpm run config-check&&cross-env NODE_ENV=development REACT_APP_DEV_API_SERVER_PORT=3500 concurrently --kill-others \"pnpm run dev-frontend\" \"pnpm run dev-backend\"",
    "st:build": "cd src/sharetribe; pnpm build-web && pnpm build-server",
    "st:build-web": "cd src/sharetribe; sharetribe-scripts build",
    "st:build-server": "cd src/sharetribe; sharetribe-scripts build-server",
    "st:format": "cd src/sharetribe; prettier --write '**/*.{js,css}'",
    "st:format-ci": "cd src/sharetribe; prettier --list-different '**/*.{js,css}'",
    "st:format-docs": "cd src/sharetribe; prettier --write '**/*.md'",
    "st:test": "cd src/sharetribe; NODE_ICU_DATA=node_modules/full-icu sharetribe-scripts test",
    "st:test-ci": "cd src/sharetribe; pnpm run test-server --runInBand && sharetribe-scripts test --runInBand",
    "st:eject": "cd src/sharetribe; sharetribe-scripts eject",
    "st:start": "cd src/sharetribe; node --icu-data-dir=node_modules/full-icu server/index.js",
    "st:dev-server": "cd src/sharetribe; cross-env-shell NODE_ENV=development PORT=4000 REACT_APP_MARKETPLACE_ROOT_URL=http://localhost:4000 \"pnpm run build&&nodemon --watch server server/index.js\"",
    "st:test-server": "cd src/sharetribe; jest ./server/**/*.test.js",
    "st:heroku-postbuild": "cd src/sharetribe; pnpm run build",
    "st:postinstall": "cd src/sharetribe; patch-package",
    "st:translate": "cd src/sharetribe; node scripts/translations.js",
    "st:docker:build": "cd src/sharetribe; docker build -t calligo-sharetribe .",
    "st:docker:run": "cd src/sharetribe; docker run -p 3000:3000 calligo-sharetribe"
  },
  "dependencies": {
    "@astrojs/check": "^0.3.1",
    "@astrojs/mdx": "^2.1.1",
    "@astrojs/react": "^3.0.10",
    "@astrojs/sitemap": "^3.0.3",
    "@astrojs/tailwind": "^5.1.0",
    "@sentry/astro": "^7.86.0",
    "@spotlightjs/astro": "^1.0.2",
    "@supabase/supabase-js": "^2.39.1",
    "@tokilabs/lang": "^1.0.7",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitest/coverage-istanbul": "^1.1.0",
    "astro": "^4.0.1",
    "astro-imagetools": "^0.9.0",
    "bootstrap": "^5.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "slick-carousel": "^1.8.1",
    "tailwindcss": "^3.0.24",
    "typescript": "^5.3.2",
    "@babel/runtime": "^7.17.9",
    "@loadable/component": "^5.15.2",
    "@loadable/server": "^5.15.2",
    "@mapbox/polyline": "^1.1.1",
    "@sentry/browser": "^6.19.7",
    "@sentry/node": "^6.19.7",
    "autosize": "^5.0.1",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.20.2",
